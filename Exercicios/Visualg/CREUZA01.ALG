Algoritmo "Creuza01"

Var
   anoAtual, anoNascimento, idade: Inteiro

Inicio
      Escreva("Em que ano nós estamos? ")
      Leia(anoAtual)
      Escreva("Em que ano eu nasci? ")
      Leia(anoNascimento)
      
      idade <- anoAtual - anoNascimento
      
      Escreva("Minha idade será ", idade)
Fimalgoritmo